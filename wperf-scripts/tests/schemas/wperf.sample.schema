{
    "type": "object",
    "required": [ "sampling" ],
    "properties": {
        "sampling": {
            "type": "object",
            "required": [ "sample_display_row", "samples_generated", "samples_dropped", "pe_file", "pdb_file", "events" ],
            "minProperties": 5,
            "properties": {
              "events": {
                	"type" : "array",
                     "items": {
                        "type": "object",
                        "required": [ "type", "samples", "interval", "printed_sample_num", "annotate" ],
                        "minProperties": 4,
                        "maxProperties": 6,
                        "properties": {
                          	"type": { "type" : "string" },
                            "samples": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "required": ["count", "overhead", "symbol"],
                                    "additionalProperties": false,
                                    "properties": {
                                        "count": { "type": "integer"},
                                        "overhead": { "type": "number" },
                                        "symbol": { "type": "string" }
                                    }
                                },
                            },
                            "annotate": {
                                "type": "array",
                                "items" : {
                                    "type": "object",
                                    "required": ["function_name", "source_code"],
                                    "additionalProperties": false,
                                    "properties": {
                                        "function_name": { "type": "string" },
                                        "source_code": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "required": ["filename", "line_number", "hits"],
                                                "additionalProperties": false,
                                                "properties": {
                                                    "filename": { "type": "string" },
                                                    "line_number": { "type": "integer" },
                                                    "hits": { "type": "integer" }
                                                }
                                            }
                                        }

                                    }

                                },
                            },
                            "pcs": {  
                            "type": "array",                      
                            "items": {
                                "type": "object",
                                "required": [ "address", "count" ],
                                "properties": {
                                    "address": { "type" : "integer" },
                                    "count": { "type": "integer" }
                                }
                            },
                            },
                            "interval": { "type": "integer" },
                            "printed_sample_num": { "type": "integer" }
                        }
                    }
              	},
                "base_address" : { "type": "integer" },
                "runtime_delta" : { "type": "integer" },
                "sample_display_row": { "type": "integer" },
                "samples_generated": { "type": "integer" },
                "pe_file": { "type" : "string" },
                "pdb_file": { "type" : "string" },
                "samples_dropped": { "type": "integer" },
                "modules": {
                  	"type": "array",
                    "items": {
                    	"type": "object",
                        "required": ["name", "address", "path"],
                        "properties": {
                          "name" : { "type": "string" },
                          "address": { "type": "integer" },
                          "path": { "type": "string" },
                        }
                    }
                },
                "modules_info": { 
                	"type" : "array",
                    "items": {
                      "type": "object",
                      "required": [ "pdb_file", "pe_name" ],
                      "properties": {
                        "pdb_file": { "type": "string" },
                        "pe_name": { "type": "string" }
                      },
                      "additionalProperties": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "required": [ "section", "offset", "virtual_size" ],
                          "properties": {
                           	"section": { "type" : "string" },
                            "offset": { "type": "integer" },
                            "virtual_size": { "type" : "integer" }
                              
                          }
                        }
                      }
                    }
                }
            }
        }
    }
}