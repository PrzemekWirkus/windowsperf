{
    "type": "object",
    "required": [ "sampling" ],
    "properties": {
        "sampling": {
            "type": "object",
            "required": [ "sample_display_row", "sample_generated", "sample_dropped" ],
            "minProperties": 4,
            "additionalProperties": {
                "type": "object",
                "required": [ "samples", "interval", "printed_sample_num", "annotate" ],
                "minProperties": 3,
                "maxProperties": 3,
                "properties": {
                    "samples": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "required": ["count", "overhead", "symbol"],
                            "additionalProperties": false,
                            "properties": {
                                "count": { "type": "integer"},
                                "overhead": { "type": "number" },
                                "symbol": { "type": "string" }
                            }
                        },
                    },
                    "annotate": {
                        "type": "array",
                        "items" : {
                            "type": "object",
                            "required": ["function_name", "source_code"],
                            "additionalProperties": false,
                            "properties": {
                                "function_name": { "type": "string" },
                                "source_code": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "required": ["filename", "line_number", "hits"],
                                        "additionalProperties": false,
                                        "properties": {
                                            "filename": { "type": "string" },
                                            "line_number": { "type": "integer" },
                                            "hits": { "type": "integer" }
                                        }
                                    }
                                }

                            }

                        },
                    },
                    "interval": { "type": "integer" },
                    "printed_sample_num": { "type": "integer" }
                }
            },
            "properties": {
                "sample_display_row": { "type": "integer" },
                "samples_generated": { "type": "integer" },
                "samples_dropped": { "type": "integer" }
            }
        }
    }
}